@model EduMax.Models.Notification

@{
    ViewBag.Title = "Get";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

    <div class="container">
        <div class="card mb-3">
            <div class="row g-0">
                <div class="col-md-4">
                    <img src="~/asset/images/favicon2.png" width="200" class="img-fluid rounded-circle" alt="...">
                </div>
                <div class="col-md-4">
                    <div class="card-body">
                        <p class="card-text"><small class="text-muted">@Model.Date</small></p>
                        <h3 class="card-title">@Model.Title</h3>
                        <p class="card-text">@Model.Message</p>
                    </div>
                </div>
            </div>
        </div>
    </div>


